<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Black">#030409</Color>
    <Color x:Key="BlackDialog">#020307</Color>
    <Color x:Key="Active">#117BEB</Color>
    <Color x:Key="White">#F8F9F9</Color>
    <Color x:Key="NotActive">#1E1E1E</Color>
    <Color x:Key="Busy">#606064</Color>
    <SolidColorBrush x:Key="BlackBrush"
                     Color="{StaticResource Black}" />
</ResourceDictionary>